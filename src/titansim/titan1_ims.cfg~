  ///////////////////////////////////////////////////////////////////////////////
  //
  //  Case:               acc1Aacc1B - acc1B
  //  Description:        10 UE access1 to access1 UDP call, Ipv6, Media enabled
  //
  ///////////////////////////////////////////////////////////////////////////////

  {
      name := "acc1Aacc1B",
      trafficCase := "CallOrig",
		callGroup := "
		publicIdPrefix=acc1;
		IdLength=5;
		IdRange=00000..00009;
                homeDomain=system.test;
                password=imt40imt40;
                privateIdPrefix=acc1;
                privateDomain=system.test;
                telIdPrefix=123456;",
      generic := {
        userWatchdog := 350.00,
        burstCalcMethod := "noMemCs"
      },
      transport := {
        {
          host := "",
          hostTransport := {
            {
              proxyTransport := {
                preferredTransport := "udp",
                transportType := IPL4, // IPL4 - use IPL4 testport, IPL2 - use LANL2 testport
                proxyList := {
                  {
                    proxyName := "3001:21::4",
                    proxyPort := 5060
                  }
                }//proxyList
              },//proxyTransport
              simulationType := {
                ueTransport := {
                  ueIpLow    := "3001:100:16::2",
                  ueIpHigh   := "3001:100:16::6",
                  uePortLow  := 10000,
                  uePortHigh := 30000
                }//ueTransport
              }//simulationType
            }
          }//hostTransport
        }
      },//transport
      trafficSpecific := {
        registration := {
          rps := 1.0,
          tempId := false,
          registrationExpires := 3600.000000,
          sbgRegMinExpires := 0.0,
          reRegMargin := 5.0,
          reRegistration := true,
          maxRetryAfter := 10,
          retryAfterFail := 30.0,
          regWatchdog := 40.0,
          PAccessNetworkInfo := "",
          includeTransportInContact := true,
          distributeRegister := false,
          delayInitialRegister := 0.0,
          authHeaderInInitialReg := false,
          deregisterCurrentContactOnly := false,
          enableSTUN := false,
          keepAliveSTUN := 30.0,
          halfcall := false
        },
        callOrig := {
          cps := 0.0,
          calledGroup := "publicIdPrefix=acc1;IdLength=5;IdRange=05000..05009;homeDomain=system.test;",
          callHoldMin := 180.0,
          callHoldMax := 180.0,
          cancelTimeMin := 0.0,
          cancelTimeMax := 0.0,
          useTelUri := false,
          maxRetryAfter := 3,
          allowMultipleEarlyDialogs := false
        },
        media :=
        {
          enabled := true,
          generator := {
            mlsim :=
            {
		enabled := true,
		enableMLSimPlus := true,
                maxNumberOfSessions := 2,
                supportedCodecs := {AMR_122_OA,H_263},
                startNumOfSessions := 1,
                simplificationLevel := 0,
                nrOfNormalSessions := 0,
                enableSessionCriteria := true
            }
          }
        }
      }
    },//trafficCase - acc1Aacc1B
